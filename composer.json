{
  "name": "emdr-therapist-finder",
  "description": "A WordPress plugin to find EMDR therapists.",
  "type": "wordpress-plugin",
  "require": {
    "php": ">=7.4",
    "guzzlehttp/guzzle": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "EMDRTherapistFinder\\": "includes/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}